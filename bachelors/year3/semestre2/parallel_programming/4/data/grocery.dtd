<?xml encoding="UTF-8"?>

<!ELEMENT stock (category)+>
<!ATTLIST stock
    xmlns CDATA #FIXED ''>

<!ELEMENT category (product)+>
<!ATTLIST category
    xmlns CDATA #FIXED ''
