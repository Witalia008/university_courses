<%= form_for @message, url: contact_path do |f| %>
    <p>
      <%= f.label :name %><br/>
      <%= f.text_field :name %>
    </p>
    <p>
      <%= f.label :email %><br/>
      <%= f.text_field :email %><br/>
    </p>
    <p>
      <%= f.label :content %><br/>
      <%= f.text_area :content, rows: "10" %>
      <%#= text_area(:message, :content, cols: 20, rows: 10) %>
    </p>
    <p><%= f.submit 'Send' %></p>
<% end %>